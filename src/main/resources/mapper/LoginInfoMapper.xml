<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sp.file.mapper.LoginInfoMapper">
	<select id="selectLoginInfoByLiId"
		resultType="com.sp.file.vo.LoginInfoVO">
		SELECT * FROM LOGIN_INFO
		WHERE Li_Id=#{liId}
	</select>
	
	<insert id="insertLoginInfo">
		INSERT INTO LOGIN_INFO(LI_ID,LI_PWD,LI_NAME)
		VALUES(#{liId}, #{liPwd}, #{liName})
	</insert>
	

</mapper>